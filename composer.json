{
    "name": "kaystrobach/typo3-polyfill-paginateviewhelper",
    "description": "Polyfill for the removed paginate ViewHelper",
    "type": "library",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Kay Strobach",
            "email": "info@kay-strobach.de"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "typo3/cms-core": "^11.5"
    },
    "autoload": {
        "classmap": [
            "Classes/*"
        ]
    },
    "config": {
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true
        }
    }
}
